	<main class="flex-fill container main-container offset-container">
		<div class="row">
			<div class="col">
				<div class="red-text page-title-text text-no-wrap">
					<span class="typer" data-type-speed="125">Contacts</span><span class="blinker">_</span>
				</div>
			</div>
		</div>

		<div class="row mt-5">
			<div class="col text-center">
				<a href="https://www.npmjs.com/~stefantigro">
					<img class="contact-link" src="/public/resources/imgs/npm-icon.svg">
				</a>
				<p class="white-text text-center contact-text">NPM</p>
			</div>

			<div class="col contact-link text-center">
				<a href="https://github.com/Michaelpalacce">
					<img class="contact-link" src="/public/resources/imgs/github-icon.svg">
				</a>
				<p class="white-text text-center contact-text">Github</p>
			</div>

			<div class="col contact-link text-center">
				<a href="https://www.linkedin.com/in/stefan-genov-286972a3/">
